{
  "dialog": {
    "version": 1,
    "type": "basic",
    "max-num-tokens": 100,
    "context": {
      "version": 1,
      "size": 4096,
      "n-vocab": 128256,
      "bos-token": 128000,
      "eos-token": [
        128001,
        128008,
        128009
      ],
      "pad-token": 128001
    },
    "sampler": {
      "version": 1,
      "seed": 42,
      "temp": 0.8,
      "top-k": 40,
      "top-p": 0.95
    },
    "tokenizer": {
      "version": 1,
      "path": "tokenizer.json"
    },
    "engine": {
      "version": 1,
      "n-threads": 3,
      "backend": {
        "version": 1,
        "type": "QnnHtp",
        "QnnHtp": {
          "version": 1,
          "spill-fill-bufsize": 0,
          "use-mmap": true,
          "mmap-budget": 0,
          "poll": true,
          "cpu-mask": "0xe0",
          "kv-dim": 128,
          "allow-async-init": true,
          "enable-graph-switching": true
        },
        "extensions": "htp_backend_ext_config.json"
      },
      "model": {
        "version": 1,
        "type": "binary",
        "positional-encoding": {
          "type": "rope",
          "rope-dim": 64,
          "rope-theta": 500000,
          "rope-scaling": {
            "rope-type": "llama3",
            "factor": 32.0,
            "low-freq-factor": 1.0,
            "high-freq-factor": 4.0,
            "original-max-position-embeddings": 8192
          }
        },
        "binary": {
          "version": 1,
          "ctx-bins": [
            "weight_sharing_model_ar1_ar128_cl4096_1_of_3.serialized.bin",
            "weight_sharing_model_ar1_ar128_cl4096_2_of_3.serialized.bin",
            "weight_sharing_model_ar1_ar128_cl4096_3_of_3.serialized.bin"
          ],
          "lora": {
            "version": 1,
            "alpha-tensor-name": "lora_alpha",
            "adapters": [
              {
                "version": 1,
                "name": "default",
                "alphas": [
                  "alpha1",
                  "alpha2"
                ],
                "bin-sections": [
                  "",
                  "ar1_cl4096_2_of_3-ar128_cl4096_2_of_3_default_adapter.bin",
                  ""
                ]
              },
              {
                "version": 1,
                "name": "elementary",
                "alphas": [
                  "alpha1",
                  "alpha2"
                ],
                "bin-sections": [
                  "",
                  "ar1_cl4096_2_of_3-ar128_cl4096_2_of_3_elementary.bin",
                  ""
                ]
              },
              {
                "version": 1,
                "name": "elementary+long",
                "alphas": [
                  "alpha1",
                  "alpha2"
                ],
                "bin-sections": [
                  "",
                  "ar1_cl4096_2_of_3-ar128_cl4096_2_of_3_elementary+long.bin",
                  ""
                ]
              },
              {
                "version": 1,
                "name": "long",
                "alphas": [
                  "alpha1",
                  "alpha2"
                ],
                "bin-sections": [
                  "",
                  "ar1_cl4096_2_of_3-ar128_cl4096_2_of_3_long.bin",
                  ""
                ]
              }
            ]
          }
        }
      }
    }
  }
}