          "allow-async-init" : false,
          "rope-theta" : 10000,
          "enable-graph-switching": false
        },
        "extensions" : "htp_backend_ext_config.json"
      },
      "model" : {
        "version" : 1,
        "type" : "binary",
        "binary" : {
          "version" : 1,
          "ctx-bins" : [
            "file_1_of_4.bin",
            "file_2_of_4.bin",
            "file_3_of_4.bin",
            "file_4_of_4.bin"
          ]
        }
      }
