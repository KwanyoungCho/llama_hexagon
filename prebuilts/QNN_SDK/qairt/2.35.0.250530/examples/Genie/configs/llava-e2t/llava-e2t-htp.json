{
  "dialog" : {
    "version" : 1,
    "type" : "basic",
    "max-num-tokens" : 200,
    "embedding" : {
      "version" : 1,
      "size" : 4096,
      "datatype" : "float32"
    },
    "context" : {
      "version" : 1,
      "size" : 2048,
      "n-vocab" : 32000,
      "bos-token" : 1,
      "eos-token" : 2
    },
    "sampler" : {
      "version" : 1,
      "seed" : 42,
      "temp" : 0.0,
      "top-k" : 1,
      "top-p" : 1.0,
      "greedy" : true
    },
    "tokenizer" : {
      "version" : 1,
      "path" : "your/path/to/tokenizer_file.json"
    },
    "engine" : {
      "version" : 1,
      "n-threads" : 3,
      "backend" : {
        "version" : 1,
        "type" : "QnnHtp",
        "QnnHtp" : {
          "version" : 1,
